public static void findLongestAndShortestWord(String[] names) {
		String min = "                          ";
		String max = "";
		for (int i=0; i<names.length; i++) {
			if (names[i].length() < min.length()) {
				min = names[i];
			}
			
			if (names[i].length() > max.length()) {
				max = names[i];
			}
		} 
			System.out.println("Shortest word is " + min );
			System.out.println("Longest word is " + max );
		
	}
